<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dcits.govsbu.sd.taxbankplatform.dkxx.mapper.BankfinalMapper">
	<!--mybatis ehcache缓存配置,以下两个<cache>标签二选一,第一个可以输出日志,第二个不输出日志 -->
	<cache type="org.mybatis.caches.ehcache.LoggingEhcache"/> 
	<!-- <cache type="org.mybatis.caches.ehcache.EhcacheCache"/> -->
	
	<resultMap id="bankfinalMap" type="com.dcits.govsbu.sd.taxbankplatform.dkxx.model.Bankfinal">
	<id column="id" property="id" jdbcType="INTEGER" />
    <result column="SQXH" property="sqxh" jdbcType="VARCHAR" />
    <result column="nsrsbh" property="nsrsbh" jdbcType="VARCHAR" />
    <result column="lf_id" property="lfId" jdbcType="INTEGER" />
    <result column="lar_pdjg" property="larPdjg" jdbcType="VARCHAR" />
    <result column="lar_credit_quota" property="larCreditQuota" jdbcType="INTEGER" />
    <result column="lar_loan_deadline" property="larLoanDeadline" jdbcType="INTEGER" />
    <result column="lar_rate" property="larRate" jdbcType="INTEGER" />
    <result column="rw_id" property="rwId" jdbcType="INTEGER" />
    <result column="create_time" property="createTime" jdbcType="TIMESTAMP" />
		
	</resultMap>

	<sql id="Base_Column_List" >
    id, SQXH, nsrsbh, lf_id, lar_pdjg, lar_credit_quota, lar_loan_deadline, lar_rate, 
    rw_id, create_time
  </sql>

	<!-- 搜索数据 -->
	<select id="queryListAll" parameterType="map" resultMap="bankfinalMap" flushCache="true" useCache="true">
		SELECT *
		FROM t_bank_final
	</select>
	
</mapper>