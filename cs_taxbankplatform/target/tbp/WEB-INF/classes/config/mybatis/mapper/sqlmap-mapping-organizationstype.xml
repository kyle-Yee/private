<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dcits.govsbu.sd.taxbankplatform.organizationstype.mapper.OrganizationsTypeMapper">
	<!--mybatis ehcache缓存配置,以下两个<cache>标签二选一,第一个可以输出日志,第二个不输出日志 -->
	<cache type="org.mybatis.caches.ehcache.LoggingEhcache"/> 

	<!-- 贷款申请信息表 -->
	<resultMap id="organizationsTypeMap" type="com.dcits.govsbu.sd.taxbankplatform.organizationstype.model.OrganizationsTypeEntity">
		<id property="id" column="ot_id"/>
		<result property="ot_type_code" column="ot_type_code" />
		<result property="ot_name" column="ot_name"/>
		<result property="enabled" column="enabled"/>
		<result property="ot_remark" column="ot_remark"/>
	</resultMap>
	
	
	<select id="queryListAll" parameterType="map" resultMap="organizationsTypeMap" flushCache="true" useCache="true">
		<!-- 查询组织类型代码 -->
		select ot_id, ot_type_code, ot_name, enabled, ot_remark from tb_organizations_type where enabled = 'Y' order by ot_id
	</select>
	
	
	<select id="findById" parameterType="String" resultMap="organizationsTypeMap" flushCache="true" useCache="true">
		<!-- 查询组织类型代码 -->
		select ot_id, ot_type_code, ot_name, enabled, ot_remark from tb_organizations_type where ot_id = #{id}
	</select>
</mapper>