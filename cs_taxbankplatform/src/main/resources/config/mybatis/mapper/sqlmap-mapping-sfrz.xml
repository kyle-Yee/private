<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dcits.govsbu.sd.taxbankplatform.auth.mapper.SfrzMapper">
	<!--mybatis ehcache缓存配置,以下两个<cache>标签二选一,第一个可以输出日志,第二个不输出日志 -->
	<cache type="org.mybatis.caches.ehcache.LoggingEhcache"/> 
	<!-- <cache type="org.mybatis.caches.ehcache.EhcacheCache"/> -->



	
	
	<select id="queryAuthInfo" parameterType="map" resultType="int"  flushCache="true" useCache="true">
			select count(*) from dj_nsrxx jb  join dj_nsrxx_kz kz on jb.djxh=kz.djxh
			where (jb.shxydm =#{nsrsbh} or jb.nsrsbh =#{nsrsbh})
			and jb.nsrmc =#{qymc} and jb.fddbrsfzjhm =#{frsfz}
			and jb.fddbrxm =#{frxm}
			and kz.fddbryddh =#{frsjh}
			
	</select>
	
	
	
	
</mapper>