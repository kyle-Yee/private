<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dcits.govsbu.sd.taxbankplatform.authorizationrecord.mapper.InterfaceLogMapper" >
<cache type="org.mybatis.caches.ehcache.LoggingEhcache"/> 
  <resultMap id="BaseResultMap" type="com.dcits.govsbu.sd.taxbankplatform.authorizationrecord.model.InterfaceLogEntity" >
    <id column="il_id" property="il_id" />
    <result column="bh" property="bh"  />
    <result column="nsrsbh" property="nsrsbh" />
    <result column="interfaceName" property="interfaceName" />
    <result column="errorLog" property="errorLog" />
    <result column="content" property="content" />
  </resultMap>

  <!-- 插入数据 -->
  <insert id="insert" parameterType="com.dcits.govsbu.sd.taxbankplatform.authorizationrecord.model.InterfaceLogEntity" >
    insert into tb_interfacelog (il_id,bh, nsrsbh,interfaceName,errorLog,content)    
    values (#{il_id},#{bh},#{nsrsbh},#{interfaceName},#{errorLog},#{content})
  </insert>
  
  
</mapper>